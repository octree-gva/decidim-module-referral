 def register(user)
      register_path = Decidim::Core::Engine.routes.url_helpers.user_registration_path
      payload = {
          tos_agreement: "1",
          email: user.email,
          name: user.name,
          password: password,
          password_confirmation: password,
          nickname: user.nickname,
          newsletter: "0", organization_id: organization.id
          }
      post(
        register_path,
        params: { user: payload, },
        headers: headers
      )
    end
